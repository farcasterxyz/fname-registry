{
  "executionRoleArn": "arn:aws:iam::526236635984:role/fname-registry-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::526236635984:role/fname-registry-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "fname-registry-api",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/fname-registry",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "api"
        }
      },
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "command": ["node", "index.js"],
      "cpu": 0,
      "dockerLabels": {
        "com.datadoghq.tags.env": "prod",
        "com.datadoghq.tags.service": "fname-registry-api"
      },
      "linuxParameters": {
        "initProcessEnabled": true
      },
      "environment": [
        {
          "name": "TINI_VERBOSITY",
          "value": "3"
        },
        {
          "name": "ENVIRONMENT",
          "value": "prod"
        },
        {
          "name": "NODE_OPTIONS",
          "value": "--enable-source-maps"
        },
        {
          "name": "DD_ENV",
          "value": "prod"
        },
        {
          "name": "DD_SERVICE",
          "value": "fname-registry-api"
        }
      ],
      "secrets": [
        {
          "valueFrom": "arn:aws:ssm:us-east-1:526236635984:parameter/FNAME_REGISTRY_MNEMONIC",
          "name": "MNEMONIC"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "fname-registry-efs",
          "containerPath": "/app/.db"
        }
      ],
      "image": "public.ecr.aws/alpine:latest",
      "essential": true
    },
    {
      "name": "fname-registry-api-datadog-agent",
      "image": "public.ecr.aws/datadog/agent:7",
      "environment": [
        {
          "name": "DD_ENV",
          "value": "prod"
        },
        {
          "name": "DD_SERVICE",
          "value": "fname-registry-api"
        },
        {
          "name": "ECS_FARGATE",
          "value": "true"
        }
      ],
      "secrets": [
        {
          "valueFrom": "arn:aws:ssm:us-east-1:526236635984:parameter/DATADOG_AGENT_API_KEY",
          "name": "DD_API_KEY"
        }
      ]
    }
  ],
  "volumes": [
    {
      "name": "fname-registry-efs",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-0f325fb3841fbadac",
        "transitEncryption": "ENABLED",
        "authorizationConfig": {
          "accessPointId": "fsap-0382d79cc19530a5f"
        }
      }
    }
  ],
  "cpu": "0.5 vCPU",
  "memory": "1024",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX",
    "cpuArchitecture": "ARM64"
  },
  "family": "fname-registry-api",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc"
}
