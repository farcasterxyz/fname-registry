    "express": "^4.18.2",
    "kysely": "^0.25.0",
    "kysely-postgres-js": "^1.1.1",
    "neverthrow": "^6.0.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "postgres": "^3.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.2.6",
    "@types/sqlite3": "^3.1.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
